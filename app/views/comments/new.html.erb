 <%= form_with(model:[@foodstall, @comment], url: foodstall_comments_path(:foodstall_id), local: true, class: "message-form") do |form| %>
     <%= form.text_field :title, placeholder: "タイトル" %>
     <%= form.text_area :text, placeholder: "コメントする", rows: "2" %>
     <%= form.submit "SEND" %>
 <% end %>
